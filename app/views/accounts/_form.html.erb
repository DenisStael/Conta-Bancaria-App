<div>
    <%= f.label :name %>
</div>
<div class="form-group">
    <%= f.text_field :name, class: "form-control" %>
</div>
<div>
    <%= f.label :birth_date %>
</div>
<div class="form-group">
    <%= f.date_select :birth_date, start_year: 1900, end_year: Time.now.year, :order => [:day, :month, :year] %>
</div>