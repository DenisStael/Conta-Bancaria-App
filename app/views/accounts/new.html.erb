<div class="jumbotron">
    <div class="container">
        <%= form_for @account do |f| %>
        <%= render "layouts/errors_messages", object: f.object %>
        <h2>Criar Conta</h2>
        <%= render "form", f: f %>
        <%= f.submit "Enviar", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>